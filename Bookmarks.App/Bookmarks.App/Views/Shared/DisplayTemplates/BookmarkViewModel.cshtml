@model Bookmarks.App.ViewModels.BookmarkViewModel
<div class="col-md-4" id="@Model.Id">
    <h2>@Model.Title</h2>
    <p>
        @Model.Description
    </p>
    @if (this.User.Identity.IsAuthenticated)
    {
        <p>
            @Html.ActionLink("Details", "Details", "Bookmarks", new {id = Model.Id}, new {@class = "btn btn-default"})
        </p>
    }
</div>
